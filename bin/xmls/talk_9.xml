    <dialog id="9" msg="你这是被我说中，没得分辨！" type="1" leftHeaderId="600201" rightHeaderId="600203" dir="1" talkName="600203">
      <option type="2" imgid="" color="" data="10" text="下一步"/>
    </dialog>
    <dialog id="10" msg="你这猴子好没心肝儿，不为人子！动辄喊打喊杀，又心性不正，要你救人，你反将人打死了，我使不起你，你去了罢！" type="1" leftHeaderId="600201" rightHeaderId="600202" dir="0" talkName="600201">
